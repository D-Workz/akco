     -->
    


    <!-- https://d-workz.github.io/hkco/#followedTask -->


    <owl:ObjectProperty rdf:about="https://d-workz.github.io/hkco/#followedTask">
        <rdfs:domain rdf:resource="https://d-workz.github.io/hkco/#ApplicationCondition"/>
        <rdfs:range rdf:resource="https://d-workz.github.io/hkco/#ApplicationTask"/>
        <rdfs:comment xml:lang="en">This is the property defines the application task that is triggered if a condition is met.</rdfs:comment>
        <rdfs:label xml:lang="en">followedTask</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://d-workz.github.io/hkco/#hasCondition -->


    <owl:ObjectProperty rdf:about="https://d-workz.github.io/hkco/#hasCondition">
        <rdfs:domain rdf:resource="https://d-workz.github.io/hkco/#ApplicationCondition"/>
        <rdfs:range rdf:resource="https://d-workz.github.io/hkco/#Condition"/>
        <rdfs:comment xml:lang="en">An application condition has specific conditions that are determined by the output of the computation. This property relates conditions to an application condition.</rdfs:comment>
        <rdfs:label xml:lang="en">hasCondition</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://d-workz.github.io/hkco/#hasDimension -->


    <owl:ObjectProperty rdf:about="https://d-workz.github.io/hkco/#hasDimension">
        <rdfs:domain rdf:resource="https://d-workz.github.io/hkco/#ApplicationTask"/>
        <rdfs:domain rdf:resource="https://d-workz.github.io/hkco/#Approach"/>
        <rdfs:domain rdf:resource="https://d-workz.github.io/hkco/#Technique"/>
        <rdfs:range rdf:resource="https://d-workz.github.io/hkco/#Dimension"/>
        <rdfs:comment xml:lang="en">Approaches and Techniques have different dimensions, i.e., characteristics. This property defines those dimensions.</rdfs:comment>
        <rdfs:label xml:lang="en">hasDimension</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://d-workz.github.io/hkco/#hasError -->


    <owl:ObjectProperty rdf:about="https://d-workz.github.io/hkco/#hasError">
        <rdfs:domain rdf:resource="https://d-workz.github.io/hkco/#KnowledgeGraph"/>
        <rdfs:range rdf:resource="https://d-workz.github.io/hkco/#Error"/>
        <rdfs:comment xml:lang="en">A knowledge graph can have certain types of errors. These errors need to be targeted by the cleaning process.</rdfs:comment>
        <rdfs:label xml:lang="en">hasError</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://d-workz.github.io/hkco/#implements -->


    <owl:ObjectProperty rdf:about="https://d-workz.github.io/hkco/#implements">
        <rdfs:domain rdf:resource="https://d-workz.github.io/hkco/#Approach"/>
        <rdfs:range rdf:resource="https://d-workz.github.io/hkco/#Technique"/>
        <rdfs:comment xml:lang="en">An approaches implements specific cleaning techniques. This property defines those used/implemented techniques.</rdfs:comment>
        <rdfs:label xml:lang="en">implements</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://d-workz.github.io/hkco/#isPublished -->


    <owl:ObjectProperty rdf:about="https://d-workz.github.io/hkco/#isPublished">
        <rdfs:domain rdf:resource="https://d-workz.github.io/hkco/#Approach"/>
        <rdfs:range rdf:resource="http://purl.org/dc/terms/BibliographicResource"/>
        <rdfs:comment xml:lang="en">An approach is published inside a publication. This property defines this publication, which is of type bibliographic resource.</rdfs:comment>
        <rdfs:label xml:lang="en">isPublished</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://d-workz.github.io/hkco/#performsTask -->


    <owl:ObjectProperty rdf:about="https://d-workz.github.io/hkco/#performsTask">
        <rdfs:domain rdf:resource="https://d-workz.github.io/hkco/#ApplicationAlgorithm"/>
        <rdfs:range rdf:resource="https://d-workz.github.io/hkco/#ApplicationTask"/>
        <rdfs:comment xml:lang="en">An application algorithm executes or performs cleaning tasks, these tasks are defined with this property. The motivation is to use the task order property, to define the order in which the properties are executed.</rdfs:comment>
        <rdfs:label xml:lang="en">performsTask</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://d-workz.github.io/hkco/#requiresQuality -->


    <owl:ObjectProperty rdf:about="https://d-workz.github.io/hkco/#requiresQuality">
        <rdfs:domain rdf:resource="https://d-workz.github.io/hkco/#Dimension"/>
        <rdfs:range rdf:resource="http://www.w3.org/ns/dqv#QualityMeasurement"/>
    </owl:ObjectProperty>
    


    <!-- https://d-workz.github.io/hkco/#targetsError -->


    <owl:ObjectProperty rdf:about="https://d-workz.github.io/hkco/#targetsError">
        <rdfs:domain rdf:resource="https://d-workz.github.io/hkco/#Dimension"/>
        <rdfs:range rdf:resource="https://d-workz.github.io/hkco/#Error"/>
    </owl:ObjectProperty>
    


    <!-- https://d-workz.github.io/hkco/#usesBackgroundKnowledge -->


    <owl:ObjectProperty rdf:about="https://d-workz.github.io/hkco/#usesBackgroundKnowledge">
        <rdfs:domain rdf:resource="https://d-workz.github.io/hkco/#Technique"/>
        <rdfs:range rdf:resource="https://d-workz.github.io/hkco/#BackgroundKnowledge"/>
    </owl:ObjectProperty>
    


